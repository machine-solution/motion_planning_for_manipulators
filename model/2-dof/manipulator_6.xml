		<material name="white" rgba="1 1 1 1"/>
		<material name="gray"  rgba=".5 .5 .5 1"/>
	</asset>

    <option collision="predefined"/>

    <worldbody>
        <light diffuse=".5 .5 .5" pos="0 0 10" dir="0 0 -1"/>
        <geom type="plane" size="2 2 0.1" rgba="1 1 1 1"/>

        <body name="link 0" pos="0.4 0 0.1" euler="0 90 0">
            <joint name="joint 0" type="hinge" axis="-1 0 0" pos="0 0 -0.4"/>
            <geom name="geom link 0" type="cylinder" size="0.05 0.4" material="red"/>
            <body name="link 1" pos="0 0 1.0" euler="0 0 0">
                <joint name="joint 1" type="hinge" axis="-1 0 0" pos="0 0 -0.6"/>
                <geom name="geom link 1" type="cylinder" size="0.05 0.6" material="red"/>
                <site name="tip" size="0.1" pos="0 0 0.6"/>
            </body>
        </body>

        <body name="link 0 shade" pos="0.4 0 0.1" euler="0 90 0">
            <joint name="joint 0 shade" type="hinge" axis="-1 0 0" pos="0 0 -0.4"/>
            <geom name="geom link 0 shade" type="cylinder" size="0.05 0.4" material="green"/>
            <body name="link 1 shade" pos="0 0 1.0" euler="0 0 0">
                <joint name="joint 1 shade" type="hinge" axis="-1 0 0" pos="0 0 -0.6"/>
                <geom name="geom link 1 shade" type="cylinder" size="0.05 0.6" material="green"/>
            </body>
        </body>

